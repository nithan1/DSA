

class Node{
    int data;
    Node next;
    Node(int x){
        data = x;
        next = null;
    }
}
class Test{
    public static void main(String[] args) {
        Node head = new Node(10);
        head.next = new Node(20);
        head.next.next = new Node(30);

        System.out.println(search(head,2) + " ");;
    }

    private static void print(Node head) {
        Node curr = head;
        while (curr!=null){
            System.out.println(curr.data + " ");
            curr = curr.next;
        }
    }

    private static int search(Node head,int x) {

        int cnt = 1;
        Node curr = head;
        while (curr!=null) {
            if (curr.data == x) {
                return cnt;
            } else {
                curr = curr.next;
                cnt++;
            }
        }
        return -1;

    }
}
