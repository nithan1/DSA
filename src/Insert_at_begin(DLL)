



class Node{
    int data;
    Node next;
    Node prev;
    Node(int x){
        data = x;
        next = null;
        prev = null;
    }
}
class Test{
    public static void main(String[] args) {

        Node head = new Node(10);
        Node temp1 = new Node(20);
        Node temp2 = new Node(30);
        head.next = temp1;
        temp1.prev = head;
        temp1.next = temp2;
        temp2.prev=temp1;
        int x = 100;
        Node ne = begin(head,x);
        print(ne);
    }

    private static Node begin(Node head,int x) {
        Node temp = new Node(x);
        temp.next = head;
        if (head!=null){
            head.prev = temp;
        }
        return temp;
    }

    private static void print(Node head) {
        Node curr = head;
        while (curr!=null){
            System.out.println(curr.data + " ");
            curr = curr.next;
        }
    }


}

