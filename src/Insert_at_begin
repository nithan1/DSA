import java.util.LinkedList;

class Node{
    int data;
    Node next;
    Node(int x){
        data = x;
        next = null;
    }
}
class LinkedLis {
    Node head;
    public void insert(int data){
        Node newNode = new Node(data);
        newNode.next = head;
        head = newNode;
    }
    public void printlist(){
        Node cur = head;
        while (cur!=null){
            System.out.println(cur.data + " ");
            cur = cur.next;
        }
    }
}
class Insert_at_begin{
    public static void main(String[] args) {
        LinkedLis list = new LinkedLis();
        list.insert(10);
        list.insert(20);
        list.insert(30);
        list.insert(40);
        list.printlist();
    }
}
